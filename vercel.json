{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "^/(.*)\\.(js|css|map|png|jpg|jpeg|svg|webp|woff2|woff|ttf)$", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "/$1.$2" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
